  <!-- Login Modal -->
  
  <div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
  
          <div class="modal-body">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="margin:15px 15px;">
                  <span aria-hidden="true">&times;</span>
                </button>
  
            <div class="panel-heading">
              <div class="font-effect-shadow-multiple heading">SIGN IN !</div>
            </div>
            <hr>
  
            <% if(JSON.parse(status) == 'login failed'){%>
              <div class="alert alert-danger alert-dismissible" style="margin-top:15px;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Sign in Failed!</strong> Please try again.
              </div>
              <% } %>
  
                <form name="loginForm" class="form-signin" role="form" action="/login" method="post">
  
                  <md-input-container class="md-block">
                    <label>Email Address</label>
                    <input required name="username" ng-model="user.username" minlength="4" maxlength="100" ng-pattern="/^.+@.+\..+$/">
  
                    <div ng-messages="loginForm.username.$error" role="alert">
                      <div ng-message-exp="['pattern']">
                        It doesn't look like an e-mail address.
                      </div>
                    </div>
                  </md-input-container>
  
                  <md-input-container class="md-block" flex-gt-sm>
                    <label>Password</label>
                    <input name="password" type="password" name="password" minlength="4" ng-model="user.password" required />
  
                    <div ng-messages="loginForm.password.$error" role="alert">
                      <div ng-message-exp="['minlength']">
                        Password is required
                      </div>
                    </div>
                  </md-input-container>
  
                  <div class="submitButton">
                    <button type="submit" class="btn btn-success btn-lg loginButton" ng-disabled="!(!!user.username && !!user.password)">Login</button>
                  </div>
                  <!-- <%=status %> -->
  
  
                </form>
          </div>
  
        </div>
      </div>
    </div>
  
    <!-- Register Modal -->
  
    <div class="modal fade" id="registerModal" tabindex="-1" role="dialog" aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
  
          <div class="modal-body">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="margin:15px 15px;">
                    <span aria-hidden="true">&times;</span>
                  </button>
            <div class="panel-heading">
              <div class="font-effect-shadow-multiple heading">REGISTER :)</div>
            </div>
            <hr>
            <% if(JSON.parse(status) == 'registration failed'){%>
              <div class="alert alert-danger alert-dismissible" style="margin-top:15px;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Registration Failed!</strong> Please try again.
              </div>
              <% } %>
            <form name="registerForm" class="form-signin" role="form" action="/register" method="post">
  
              <md-input-container class="md-block">
                <label>Name</label>
                <input name="name" ng-model="user.name">
              </md-input-container>
  
  
              <md-input-container class="md-block">
                <label>Email Address</label>
                <input required name="username" ng-model="user.username" minlength="4" maxlength="100" ng-pattern="/^.+@.+\..+$/">
  
                <div ng-messages="registerForm.username.$error" role="alert">
                  <div ng-message-exp="['pattern']">
                    It doesn't look like an e-mail address.
                  </div>
                </div>
              </md-input-container>
  
              <md-input-container class="md-block" flex-gt-sm>
                <label>Password</label>
                <input name="password" type="password" name="password" ng-model="user.password" required/>
                <div ng-messages="registerForm.password.$error" role="alert">
                  <div ng-message="required">Password is required </div>
                </div>
              </md-input-container>
  
              <!-- Confirm password block -->
              <md-input-container class="md-block">
                <label>Confirm Password</label>
                <input type="password" name="confirm_password" ng-model="user.confirm_password" compare-to="user.password" required/>
                <div ng-messages="registerForm.confirm_password.$error" role="alert" class="form-errors first-name-error">
                  <div ng-message="required">Password is required.</div>
                  <div ng-message="compareTo">Passwords are not matching</div>
                </div>
              </md-input-container>
  
              <md-input-container class="md-block">
                <label>University</label>
                <input name="university" ng-model="user.university">
              </md-input-container>
  
              <md-input-container class="md-block">
                <label>Describe yourself</label>
                <textarea name="description" ng-model="user.description" md-maxlength="150" rows="1" placeholder="Your skillset, experience and technical interests"
                  md-select-on-focus></textarea>
              </md-input-container>
  
              <!-- <label for="comment">Describe yourself</label>
              <textarea class="form-control" rows="5" id="description" placeholder="Your skillset, experience and technical interests></textarea> -->
  
              <div class="submitButton">
                <button type="submit" class="btn btn-primary btn-lg loginButton" ng-disabled="!(!!user.username && !!user.password && user.confirm_password)">Register</button>
              </div>
            </form>
          </div>
  
        </div>
      </div>
    </div>
  
